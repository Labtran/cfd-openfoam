FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          sample;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPoint;

setFormat raw;

sets
(
    linhaA
    {
        type            lineCell;
        axis            distance;//x;
        start           (0    0.05  0.005);
        end             (0.1  0.05  0.005);
    }
    linhaB
    {
        type            lineCell;
        axis            distance;//y;
        start           (0.055 0   0.005);
        end             (0.055 0.1 0.005);
    }
);

fields
(
    U
);


