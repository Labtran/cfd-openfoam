FoamFile
{
    version     10.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

divSchemes
{
    default         Gauss linear;
}

gradSchemes
{
    default         Gauss linear;
}	

laplacianSchemes
{
    default         Gauss linear corrected;
}
