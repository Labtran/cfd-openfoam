FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          sample;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPointFace;

setFormat raw;

sets
(
    linhaA
    {
        type            lineUniform;
        nPoints		5;
        axis            y;
        start           (0.2  0    0.05);
        end             (0.2  0.1  0.05);
    }
);

fields
(
    U
);


